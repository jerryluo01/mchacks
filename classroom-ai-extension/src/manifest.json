{
    "manifest_version": 3,
    "name": "Classroom AI",
    "version": "0.1.0",
    "description": "Test",
    "permissions": ["storage", "identity"],
    "background": {
      "service_worker": "background/background.js",
      "type": "module"
    },
    "action": {
      "default_title": "Classroom AI"
    },
    "content_scripts": [
      {
        "matches": ["https://classroom.google.com/*"],
        "js": ["content/classroomDetector.js"],
        "run_at": "document_idle"
      }
    ]
  }
  